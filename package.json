  "description": "starting scene",
  "main": "app.js",
  "scripts": {
    "build": "webpack --mode development",
    "dev": "npm run build && webpack-dev-server --port=8080 --hot --inline",
    "start": "npm run build && webpack-dev-server --port=8080 --hot --inline",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "css-build": "node-sass --omit-source-map-url src/Scripts/Style/config.scss dist/css/global_style.css",
    "css-watch": "npm run css-build -- --watch"
